<%= simple_form_for @admin_metum do |f| %>
  <div class="form-group">
    <%= f.input :title %>
  </div>
  <div class="form-group">
    <%= f.input :meta_description, as: :text %>
  </div>
  <div class="form-group">
    <%= f.input :og_title %>
  </div>
  <div class="form-group">
    <%= f.input :og_description, as: :text%>
  </div>
  <div class="form-group">
    <%= f.input :og_image, input_html: {'data-default-file': @admin_metum.og_image.url,class: 'dropify'} %>
  </div>
  <div class="form-group mt-xs-20">
    <%= f.button :submit, class: 'form-control' %>
  </div>
  <div class="form-group">
  <%= link_to "Cancel", admin_meta_path, class: "btn btn-default form-control"%>
  </div>
<% end %>